{
  "name": "react-express-auth-template",
  "version": "1.0.0",
  "description": "This repo can be used to start a React+Express project fully equipped with Auth for user creation and login.",
  "main": "server/index.js",
  "scripts": {
    "kickstart": "cd frontend && npm i && npm run build && cd ../server && npm i && npm run migrate && npm run seed",
    "build:frontend": "cd frontend && npm i && npm run build",
    "migrate": "cd server && npm run migrate",
    "seed": "cd server && npm run seed",
    "start": "cd server && npm start",
    "dev": "cd server && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "connect": "psql -U postgres -d react_auth_example",
    "lint": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@react-three/drei": "^10.0.8",
    "@react-three/fiber": "^9.1.2",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "react-datepicker": "^7.5.0",
    "three": "^0.176.0"
  }
}
